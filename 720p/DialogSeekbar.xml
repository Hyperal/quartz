<window type="dialog" id="115">
  <defaultcontrol always="true">901</defaultcontrol>
  <allowoverlay>yes</allowoverlay>
  <include>OpenClose</include>
  <visible>![Window.IsVisible(SliderDialog) | Window.IsVisible(OSDVideoSettings) | Window.IsVisible(OSDAudioSettings) | Window.IsVisible(VideoBookmarks)]</visible>
  <visible>Window.IsVisible(FullScreenVideo) + [Window.IsActive(videoosd.xml) | Player.Seeking | Player.DisplayAfterSeek | Player.Paused | Player.Forwarding | Player.Rewinding | Player.Caching]</visible>
  <animation effect="fade" start="0" end="100" time="400">Visible</animation>
  <animation effect="fade" start="100" end="0" time="50">Hidden</animation>
  <controls>
    <control type="group" description="Top Bar">
      <visible>!Window.IsActive(142)</visible>
      <animation effect="fade" start="0" end="100" time="400">Visible</animation>
      <animation effect="fade" start="100" end="0" time="50">Hidden</animation>
      <control type="image" description="Background">
        <posx>0</posx>
        <posy>0</posy>
        <width>1280</width>
        <height>40</height>
        <texture>img/BlackDotST.png</texture>
      </control>
      <control type="label" description="Clock">
        <posx>40</posx>
        <posy>0</posy>
        <width>130</width>
        <height>40</height>
        <align>left</align>
        <aligny>center</aligny>
        <font>size18</font>
        <textcolor>Silver</textcolor>
        <label>$INFO[System.Time]</label>
      </control>
      <control type="label" description="End time">
        <posx>40r</posx>
        <posy>0</posy>
        <width>130</width>
        <height>40</height>
        <align>right</align>
        <aligny>center</aligny>
        <font>size18</font>
        <textcolor>Silver</textcolor>
        <label>$INFO[Player.FinishTime,(,)]</label>
      </control>
    </control>
    <control type="group">
      <posy>180</posy>
      <control type="image" description="Background">
        <visible>player.forwarding | player.rewinding | [Player.Paused + Player.Caching] | [Player.DisplayAfterSeek + !IsEmpty(Player.SeekOffset)]</visible>
        <posx>570</posx>
        <posy>350</posy>
        <width>140</width>
        <height>50</height>
        <texture border="6">img/BlackRounded.png</texture>
      </control>
      <control type="label" id="24" description="Buffering label">
        <visible>[Player.Paused + Player.Caching] + !Player.Seeking</visible>
        <include>SeekbarLabels</include>
        <label>$INFO[Player.CacheLevel]%</label>
      </control>
      <control type="label" description="2x RW FW label">
        <visible>player.forwarding2x | player.rewinding2x</visible>
        <include>SeekbarLabels</include>
        <label>2 X</label>
      </control>
      <control type="label" description="4x RW FW label">
        <visible>player.forwarding4x | player.rewinding4x</visible>
        <include>SeekbarLabels</include>
        <label>4 X</label>
      </control>
      <control type="label" description="8x RW FW label">
        <visible>player.forwarding8x | player.rewinding8x</visible>
        <include>SeekbarLabels</include>
        <label>8 X</label>
      </control>
      <control type="label" description="16x RW FW label">
        <visible>player.forwarding16x | player.rewinding16x</visible>
        <include>SeekbarLabels</include>
        <label>16 X</label>
      </control>
      <control type="label" description="32x RW FW label">
        <visible>player.forwarding32x | player.rewinding32x</visible>
        <include>SeekbarLabels</include>
        <label>32 X</label>
      </control>
      <control type="label" description="Seek time label">
        <visible>![player.forwarding | player.rewinding]</visible>
        <visible>Player.DisplayAfterSeek</visible>
        <include>SeekbarLabels</include>
        <label>$INFO[Player.SeekOffset]</label>
      </control>
    </control>
    <control type="group" description="Dialog Background">
      <animation type="Conditional" condition="Window.IsActive(videoosd.xml)">
        <effect type="slide" start="0,0" end="0,-45" time="400" tween="cubic" easing="inout" reversible="true" />
      </animation>
      <control type="image">
        <posx>0</posx>
        <posy>650</posy>
        <width>1280</width>
        <height>150</height>
        <aspectratio>stretch</aspectratio>
        <texture>VideoOSD/BGVideoOSD.png</texture>
      </control>
    </control>
    <control type="label">
      <include>DialogID</include>
      <label>DialogSeekbar.xml</label>
    </control>
    <control type="group">
      <posy>685</posy>
      <control type="image" description="Real Background">
        <posx>200</posx>
        <posy>4</posy>
        <width>936</width>
        <height>16</height>
        <texture border="15,0,15,0">VideoOSD/ProgressRealBG.png</texture>
      </control>
      <control type="progress" description="Buffer Bar">
        <posx>200</posx>
        <posy>4</posy>
        <width>924</width>
        <height>16</height>
        <texturebg border="8">VideoOSD/VideoProgressBG.png</texturebg>
        <midtexture border="0,8,0,8">VideoOSD/BufferMidd.png</midtexture>
        <lefttexture border="6,8,0,8">VideoOSD/BufferLeft.png</lefttexture>
        <righttexture border="0,8,16,8">VideoOSD/BufferRight.png</righttexture>
        <info>Player.CacheLevel</info>
        <visible>Player.Caching</visible>
      </control>
      <control type="progress" id="23" description="Progress Bar">
        <posx>200</posx>
        <posy>4</posy>
        <width>938</width>
        <height>16</height>
        <texturebg border="8">VideoOSD/VideoProgressBG.png</texturebg>
        <midtexture border="0,8,0,8">VideoOSD/VideoProgressMidd.png</midtexture>
        <lefttexture border="6,8,0,8">VideoOSD/VideoProgressLeft.png</lefttexture>
        <righttexture border="0,8,16,8">VideoOSD/VideoProgressRight.png</righttexture>
        <info>Player.Progress</info>
        <visible>true</visible>
      </control>
      <control type="image" description="Background Overlay Left">
        <posx>172</posx>
        <posy>4</posy>
        <width>36</width>
        <height>16</height>
        <texture>VideoOSD/VideoProgressOverlayL.png</texture>
      </control>
      <control type="image" description="Background Overlay Right">
        <posx>1132</posx>
        <posy>4</posy>
        <width>12</width>
        <height>16</height>
        <texture>VideoOSD/VideoProgressOverlayR.png</texture>
      </control>
      <control type="label">
        <posx>100</posx>
        <posy>0</posy>
        <height>20</height>
        <width>120</width>
        <align>left</align>
        <label>$INFO[Player.Time]</label>
        <font>size17</font>
      </control>
      <control type="label">
        <posx>40r</posx>
        <posy>0</posy>
        <height>20</height>
        <width>120</width>
        <align>right</align>
        <label>$INFO[Player.TimeRemaining,-]</label>
        <font>size17</font>
      </control>
    </control>
    <control type="group">
      <posy>681</posy>
      <posx>0</posx>
      <control type="image" description="Buffer icon">
        <visible>Player.Caching</visible>
        <posx>35</posx>
        <include>SeekbarIconsCommons</include>
        <texture border="2">busy/Busy.png</texture>
        <animation effect="rotate" start="360" end="0" center="auto" time="1200" loop="true" pulse="true" condition="true">conditional</animation>
      </control>
      <control type="image" description="Pause">
        <visible>Player.Paused + !Player.Caching</visible>
        <posx>35</posx>
        <include>SeekbarIconsCommons</include>
        <texture border="2">OSD/OSDProgressPause.png</texture>
      </control>
      <control type="image" description="Play">
        <visible>player.playing | player.forwarding2x | player.forwarding4x | player.forwarding8x | player.forwarding16x | player.forwarding32x</visible>
        <posx>35</posx>
        <include>SeekbarIconsCommons</include>
        <texture border="2">OSD/OSDProgressPlay.png</texture>
      </control>
      <control type="image" description="FF 2">
        <visible>player.forwarding2x | player.forwarding4x | player.forwarding8x | player.forwarding16x | player.forwarding32x</visible>
        <posx>50</posx>
        <include>SeekbarIconsCommons</include>
        <texture border="2">OSD/OSDProgressPlay.png</texture>
      </control>
      <control type="image" description="RW 2">
        <visible>player.rewinding2x | player.rewinding4x | player.rewinding8x | player.rewinding16x | player.rewinding32x</visible>
        <posx>35</posx>
        <include>SeekbarIconsCommons</include>
        <texture flipx="true" border="2">OSD/OSDProgressPlay.png</texture>
      </control>
      <control type="image" description="RW 2">
        <visible>player.rewinding2x | player.rewinding4x | player.rewinding8x | player.rewinding16x | player.rewinding32x</visible>
        <posx>50</posx>
        <include>SeekbarIconsCommons</include>
        <texture flipx="true" border="2">OSD/OSDProgressPlay.png</texture>
      </control>
    </control>
    
    <!--<control type="image" description="Progress back">
				<posx>5</posx>
				<posy>39</posy>
				<width>1130</width>
				<height>31</height>
				<texture flipy="false" border="4">img/ProgressBack2.png</texture>
			</control>-->
    
    <!--<control type="slider" id="401">
				<description>Seekbar</description>
				<posx>40</posx>
				<posy>50</posy>
				<width>1200</width>
				<height>10</height>
				<visible>Player.Seeking</visible>
			</control>-->
    
    <include>Debug_Grid</include>
  </controls>
</window>
