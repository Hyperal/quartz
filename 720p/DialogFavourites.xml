<window type="dialog" id="134">
  <defaultcontrol always="true">450</defaultcontrol>
  <allowoverlay>no</allowoverlay>
  <include>OpenClose</include>
  <coordinates>
    <posx>0</posx>
    <posy>0</posy>
  </coordinates>
  <controls>
    <include>MiniBackground</include>
    <control type="label" description="Header label">
      <include>HeadingR</include>
      <label>1036</label>
    </control>
    <control type="group" description="Slider and List">
      <animation effect="slide" start="0,0" end="0,-222" time="0" condition="StringCompare(Container(450).NumItems,1)">conditional</animation>
      <animation effect="slide" start="0,0" end="0,-148" time="0" condition="StringCompare(Container(450).NumItems,2)">conditional</animation>
      <animation effect="slide" start="0,0" end="0,-148" time="0" condition="StringCompare(Container(450).NumItems,3)">conditional</animation>
      <animation effect="slide" start="0,0" end="0,-74" time="0" condition="StringCompare(Container(450).NumItems,4)">conditional</animation>
      <animation effect="slide" start="0,0" end="0,-74" time="0" condition="StringCompare(Container(450).NumItems,5)">conditional</animation>
      <posx>680</posx>
      <posy>94</posy>
      <control type="fixedlist" id="450">
        <posx>0</posx>
        <posy>0</posy>
        <width>535</width>
        <height>592</height>
        <onleft>-</onleft>
        <onright condition="Skin.HasSetting(Scrollbars_Active)">60</onright>
        <onup>450</onup>
        <ondown>450</ondown>
        <pagecontrol>60</pagecontrol>
        <focusposition>3</focusposition>
        <movement>4</movement>
        <scrolltime tween="sine" easing="inout">280</scrolltime>
        <itemlayout width="580" height="74">
          <control type="image">
            <posx>0</posx>
            <posy>2</posy>
            <height>70</height>
            <width>580</width>
            <texture>img/ListBG.png</texture>
          </control>
          <control type="image">
            <posx>0</posx>
            <posy>0</posy>
            <height>74</height>
            <width>74</width>
            <bordersize>6</bordersize>
            <texture background="true">$INFO[ListItem.Icon]</texture>
            <aspectratio>scale</aspectratio>
          </control>
          <control type="image">
            <posx>0</posx>
            <posy>0</posy>
            <height>74</height>
            <width>74</width>
            <bordersize>6</bordersize>
            <texture border="3">img/CoverOverlay0.png</texture>
          </control>
          <control type="label" description="Main Label">
            <scroll>false</scroll>
            <textcolor>White</textcolor>
            <posx>90</posx>
            <posy>0</posy>
            <width>470</width>
            <height>74</height>
            <label>$INFO[ListItem.Label]</label>
          </control>
        </itemlayout>
        <focusedlayout width="580" height="74">
          <control type="image">
            <posx>0</posx>
            <posy>2</posy>
            <height>70</height>
            <width>580</width>
            <texture>img/ListBG.png</texture>
          </control>
          <control type="image">
            <posx>0</posx>
            <posy>0</posy>
            <height>74</height>
            <width>74</width>
            <bordersize>6</bordersize>
            <texture background="true">$INFO[ListItem.Icon]</texture>
            <aspectratio>scale</aspectratio>
          </control>
          <control type="image">
            <posx>0</posx>
            <posy>0</posy>
            <height>74</height>
            <width>74</width>
            <bordersize>6</bordersize>
            <texture border="3">img/CoverOverlay0.png</texture>
          </control>
          <control type="label" description="Main Label">
            <posx>90</posx>
            <posy>0</posy>
            <width>470</width>
            <height>74</height>
            <textcolor>White2</textcolor>
            <label>$INFO[ListItem.Label]</label>
          </control>
        </focusedlayout>
      </control>
      <control type="group" description="Slider">
        <animation condition="Control.HasFocus(60)" effect="slide" start="0,0" end="640,360" time="300" reversible="true">Conditional</animation>
        <animation condition="Control.HasFocus(60)" effect="zoom" start="100" end="0" time="300" reversible="true">Conditional</animation>
        <control type="image">
          <animation effect="fade" time="100">VisibleChange</animation>
          <posx>-23</posx>
          <posy>200</posy>
          <width>584</width>
          <height>119</height>
          <texture border="35">img/Slider4.png</texture>
          <aspectratio>stretch</aspectratio>
        </control>
        <animation type="Conditional" condition="ControlGroup(9000).HasFocus()">
          <effect type="fade" start="100" end="40" time="0" reversible="true" />
        </animation>
        <animation condition="Container(450).Position(0)" effect="slide" start="0,0" end="0,-222"
					time="180" easing="inout" reversible="true">Conditional</animation>
        <animation condition="Container(450).Position(1)" effect="slide" start="0,0" end="0,-148"
					time="180" easing="inout" reversible="true">Conditional</animation>
        <animation condition="Container(450).Position(2)" effect="slide" start="0,0" end="0,-74"
					time="180" easing="inout" reversible="true">Conditional</animation>
        <animation condition="Container(450).Position(3)" effect="slide" start="0,0" end="0,0"
					time="180" easing="inout" reversible="true">Conditional</animation>
        <animation condition="Container(450).Position(4)" effect="slide" start="0,0" end="0,74"
					time="180" easing="inout" reversible="true">Conditional</animation>
        <animation condition="Container(450).Position(5)" effect="slide" start="0,0" end="0,148"
					time="180" easing="inout" reversible="true">Conditional</animation>
        <animation condition="Container(450).Position(6)" effect="slide" start="0,0" end="0,222"
					time="180" easing="inout" reversible="true">Conditional</animation>
        <animation condition="Container(450).Position(7)" effect="slide" start="0,0" end="0,296"
					time="180" easing="inout" reversible="true">Conditional</animation>
        <animation condition="Container(450).Position(8)" effect="slide" start="0,0" end="0,370"
					time="180" easing="inout" reversible="true">Conditional</animation>
      </control>
    </control>
    <control type="image">
      <visible>Container.HasPrevious</visible>
      <animation effect="fade" time="200">VisibleChange</animation>
      <posx>670</posx>
      <posy>90</posy>
      <height>54</height>
      <width>555</width>
      <texture flipy="true">img/DefaultGradient.png</texture>
      <aspectratio>stretch</aspectratio>
    </control>
    <control type="image">
      <animation effect="fade" time="200">VisibleChange</animation>
      <visible>Container.HasNext</visible>
      <posx>670</posx>
      <posy>642</posy>
      <height>54</height>
      <width>555</width>
      <texture>img/DefaultGradient.png</texture>
      <aspectratio>stretch</aspectratio>
    </control>
    <control type="group" description="Thumbs">
      <posx>-4</posx>
      <posy>178</posy>
      <!--<animation effect="rotatey" start="0" end="-9" center="260," time="0" reversible="false" condition="true">Conditional</animation>-->
      <control type="group" description="Squares">
        <control type="image" description="Thumb Reflection">
          <posx>165</posx>
          <posy>390</posy>
          <width>330</width>
          <height>330</height>
          <include>Icon_Fade</include>
          <texture background="true" diffuse="img/Reflection.png" flipy="true">$INFO[ListItem.Icon]</texture>
          <aspectratio>scale</aspectratio>
        </control>
        <control type="image" description="Thumb">
          <posx>165</posx>
          <posy>60</posy>
          <width>330</width>
          <height>330</height>
          <include>Icon_Fade</include>
          <texture diffuse="img/DiffuseB.png" background="true">$INFO[ListItem.Icon]</texture>
          <aspectratio>scale</aspectratio>
        </control>
        <control type="image" description="Thumb">
          <posx>165</posx>
          <posy>60</posy>
          <width>330</width>
          <height>330</height>
          <include>Icon_Fade</include>
          <texture border="3">img/CoverOverlay0.png</texture>
          <aspectratio>stretch</aspectratio>
        </control>
      </control>
    </control>
    <control type="scrollbar" id="60">
      <posx>34r</posx>
      <posy>90</posy>
      <height>580</height>
      <onleft>450</onleft>
      <onright>-</onright>
    </control>
    <include>Debug_Grid</include>
  </controls>
</window>
