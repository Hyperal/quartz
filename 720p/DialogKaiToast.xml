<window id="107">
  <type>dialog</type>
  <zorder>2</zorder>
  <allowoverlay>no</allowoverlay>
  <animation type="WindowOpen">
    <effect type="fade" start="0" end="100" time="240"/>
    <effect type="zoom" tween="cubic" easing="out" start="0" end="100" center="640,50" time="240"/>
  </animation>
  <animation type="WindowClose">
    <effect type="fade" start="100" end="0" time="180"/>
    <effect type="zoom" tween="cubic" easing="in" start="100" end="0" center="640,50" time="180"/>
  </animation>
  <controls>
    <control type="label">
      <posy>38</posy>
      <include>DialogID</include>
      <label>DialogKaiToast.xml</label>
    </control>
    <control type="image" description="background">
      <posx>440</posx>
      <posy>4</posy>
      <width>400</width>
      <height>60</height>
      <aspectratio>stretch</aspectratio>
      <texture border="12">img/KaiDialogBG.png</texture>
    </control>
    <control type="group">
      <posx>370</posx>
      <posy>12</posy>
      <control type="group">
        <posx>80</posx>
        <posy>2</posy>
        <control type="image" id="400" description="File Icon">
          <posx>0</posx>
          <posy>0</posy>
          <width>40</width>
          <height>40</height>
          <aspectratio align="left" aligny="center">keep</aspectratio>
          <texture>DefaultIconInfo.png</texture>
        </control>
        <control type="image" id="403" description="Default Icon">
          <posx>0</posx>
          <posy>0</posy>
          <width>40</width>
          <height>40</height>
          <aspectratio align="left" aligny="center">keep</aspectratio>
          <visible>false</visible>
          <texture>DefaultIconInfo.png</texture>
        </control>
        <control type="image" id="404" description="Default Warning">
          <posx>0</posx>
          <posy>0</posy>
          <width>40</width>
          <height>40</height>
          <aspectratio align="left" aligny="center">keep</aspectratio>
          <visible>false</visible>
          <texture>DefaultIconWarning.png</texture>
        </control>
        <control type="image" id="405" description="Default Error">
          <posx>0</posx>
          <posy>0</posy>
          <width>40</width>
          <height>40</height>
          <aspectratio>keep</aspectratio>
          <visible>false</visible>
          <texture>DefaultIconError.png</texture>
        </control>
        <control type="label" id="401" description="Heading">
          <posx>50</posx>
          <posy>-4</posy>
          <width>320</width>
          <height>18</height>
          <font>size17</font>
          <align>left</align>
          <aligny>bottom</aligny>
          <!--<textcolor>Silver</textcolor>-->
          <scroll>true</scroll>
          <label>-</label>
        </control>
        <control type="label" id="402" description="Message">
          <posx>50</posx>
          <posy>18</posy>
          <width>320</width>
          <height>18</height>
          <scroll>true</scroll>
          <font>size17</font>
          <!--<textcolor>Silver</textcolor>-->
          <align>left</align>
          <aligny>bottom</aligny>
          <label>-</label>
        </control>
      </control>
    </control>
    <include>Debug_Grid</include>
  </controls>
</window>
